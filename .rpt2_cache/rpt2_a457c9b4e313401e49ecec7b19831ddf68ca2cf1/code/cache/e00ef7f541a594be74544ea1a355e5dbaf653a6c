nChooseNumber(evt) {\r\n        console.log(evt);\r\n        this._chooseGrid.title = evt;\r\n    }\r\n    check() {\r\n        let result = Toolkit.makeMatrix(), j = 0;\r\n        for (let i = 0; i < 81; i++) {\r\n            const item = this._list.getChildAt(i);\r\n            const title = +item.title || 0;\r\n            result[j][i % 9] = title;\r\n            if ((i + 1) % 9 == 0) {\r\n                j++;\r\n            }\r\n        }\r\n        console.log(result);\r\n    }\r\n}\r\n",
